<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<head>
    <style>
        BODY { margin: 30px;
         background-color: #FFFFF0}
        input {
        	width: 300px;
        	font-size: 13px;
        	padding: 3px 0 4px 10px;
        	border: 1px solid #cecece;
        	background: #F6F6f6;
        	border-radius: 8px;
        }
        label {
            font-size: 16px;
            font-weight: 700;
             cursor: pointer;
        }
        button {
    	    cursor: pointer;
    	    border: 1px solid #cecece;
    	    background: #f6f6f6;
    	    box-shadow: inset 0px 20px 20px #ffffff;
    	    border-radius: 8px;
    	    padding: 8px 14px;
    	    width: 160px;
        }
        tr:nth-child(odd) {
          background-color: #f6f6f6;
        }
        th, td {
          text-align: left;
          padding: 5px;
          font-size: 18px;
        }
        table {
          border-collapse: collapse;
          width: 50%;
        }
    </style>
</head>

